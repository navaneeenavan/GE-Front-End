{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
     "jsx": "react-jsx",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts", "app/pages/MyPatients.jsx", "app/(tabs)/_layout.tsx",
  ]
}
